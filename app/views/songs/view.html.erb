<a href="/songs/show">Dashboard</a>
<a href="/users/logout">Logout</a>
<h3>Other who added <%= @song.artist %> - <%= @song.title %></h3>
<table>
    <tr>
        <th>Name</th>
        <th>Times Added</th>
    </tr>
    <% @users.each do |user| %>
    <tr>
        <td><a href="/users/<%= user.id %>"><%= user.first_name %> <%= user.last_name %> </a></td>
        <td><%= user.plays.where(song_id: @song.id).count %></td> 
    </tr>
    <% end %>
    </table>